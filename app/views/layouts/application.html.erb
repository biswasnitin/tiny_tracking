<!DOCTYPE html>
<html>
  <head>
    <title>Tinytracking</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if @current_user%>
    
    Logged in as: <%=@current_user.email %> <%= link_to 'Logout', logout_path %> &nbsp; &nbsp; <%= link_to 'User List', users_path %>
    <%end%>
    <%= yield %>

  </body>
</html>

<style>
.redfont{
    color:red;
}
</style>
