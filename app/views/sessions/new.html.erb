<h1>Login</h1>



<%=form_tag sessions_path do %>

<%if flash[:error]%>
<strong><p><font color ="red"><%= flash[:error] %></font></p></strong>

<% end %>
  <p>
    <%= label_tag :email %><br/>
    <%= text_field_tag :email,params[:email] %>

  </p>
  <p>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>

  </p>

  
  <p>
    <%=  submit_tag 'Log in' %>
  </p>


<%  end %>
